{
  "name": "@marmota/marmodo",
  "version": "0.0.1",
  "description": "Parser for marmota-flavored-markdown (MfM)",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "compile": "tsc",
    "test": "jest ./test",
    "test:watch": "jest --watch ./test",
    "test:debug": "node --inspect-brk ../node_modules/jest/bin/jest.js --runInBand ./test",
    "doc": "jsdoc src --tutorials docs -r -c .jsdoc.conf.json --readme README.md",
    "build": "tsc --declaration --outDir dist --project tsconfig.build.json"
  },
  "author": "David Tanzer (business@davidtanzer.net)",
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/jsdom": "^21.1.7",
    "better-docs": "^2.7.3",
    "docdash": "^2.0.2",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.3",
    "jsdom": "^25.0.1",
    "omnimock": "^1.0.0-rc.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
  }
}
